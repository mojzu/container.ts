{
  "rulesDirectory": [
    "node_modules/tslint-eslint-rules/dist/rules"
  ],
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules"
  ],
  "rules": {
    "ordered-imports": [
      false
    ],
    "no-var-requires": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "max-classes-per-file": [
      false
    ],
    "object-literal-sort-keys": false
  }
}
