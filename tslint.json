{
  "rulesDirectory": [
    "node_modules/tslint-eslint-rules/dist/rules"
  ],
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules"
  ],
  "rules": {
    "no-var-requires": false,
    "no-empty": false,
    "no-string-literal": false,
    "object-literal-sort-keys": false,
    "ordered-imports": [
      false
    ],
    "max-line-length": [
      true,
      140
    ],
    "variable-name": [
      true,
      "ban-keywords"
    ],
    "max-classes-per-file": [
      false
    ]
  }
}
