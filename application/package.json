{
  "name": "application.ts",
  "version": "0.0.0-development",
  "description": "TypeScript application template",
  "license": "Unlicense",
  "author": "mojzu <mail@mojzu.net>",
  "repository": "https://github.com/mojzu/templates.ts.git",
  "private": true,
  "bin": "./dist/main.js",
  "scripts": {
    "clean": "rimraf build coverage dist",
    "distclean": "npm run clean && rimraf node_modules docs",
    "test": "npm run tsc && istanbul cover test.js -x \"**/*.spec.js\"",
    "lint": "tslint -p tsconfig.json --type-check",
    "docs": "typedoc --out docs .",
    "start": "npm run tsc && node dist/main.js",
    "inspect": "npm run tsc && node --inspect dist/main.js",
    "build": "npm run webpack && pkg --targets node6 --out-path build .",
    "tsc": "npm run clean && tsc",
    "webpack": "npm run clean && webpack"
  },
  "pkg": {
    "scripts": "scripts/**/*.js",
    "assets": "assets/**/*"
  },
  "dependencies": {
    "rxjs": "^5.4.1"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.53",
    "@types/node": "^6.0.78",
    "istanbul": "^0.4.5",
    "jasmine": "^2.6.0",
    "pkg": "^4.1.1",
    "rimraf": "^2.6.1",
    "ts-loader": "^2.2.1",
    "tslint": "^5.4.3",
    "tslint-eslint-rules": "^4.1.1",
    "typedoc": "^0.7.1",
    "typescript": "~2.3.4",
    "webpack": "^3.0.0"
  }
}
