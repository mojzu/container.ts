{
  "name": "container.ts",
  "version": "3.1.2",
  "description": "Modular application framework",
  "main": "./index.js",
  "typings": "./index.d.ts",
  "repository": "https://github.com/mojzu/container.ts.git",
  "author": "mojzu <mail@mojzu.net>",
  "license": "Unlicense",
  "files": [
    "/index.d.ts",
    "/index.js",
    "/index.js.map",
    "/container/**/!(*.test).d.ts",
    "/container/**/!(*.test).js",
    "/container/**/!(*.test).js.map",
    "/lib/**/!(*.test).d.ts",
    "/lib/**/!(*.test).js",
    "/lib/**/!(*.test).js.map"
  ],
  "scripts": {
    "clean": "ts-node fuse.ts clean",
    "distclean": "ts-node fuse.ts distclean",
    "lint": "ts-node fuse.ts lint",
    "test": "ts-node fuse.ts test",
    "example": "ts-node fuse.ts example",
    "dist": "ts-node fuse.ts dist"
  },
  "dependencies": {
    "@types/debug": "^0.0.30",
    "@types/lodash": "^4.14.116",
    "awilix": "^3.0.9",
    "debug": "^4.0.1",
    "lodash": "^4.17.11",
    "rxjs": "^6.3.3"
  },
  "optionalDependencies": {
    "@types/luxon": "^1.2.2",
    "@types/validator": "^9.4.2",
    "luxon": "^1.4.2",
    "validator": "^10.8.0"
  },
  "devDependencies": {
    "@types/del": "^3.0.1",
    "@types/jest": "^23.3.2",
    "@types/node": "^10.11.3",
    "@types/yargs": "^12.0.1",
    "del": "^3.0.0",
    "fuse-box": "^3.5.0",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1",
    "yargs": "^12.0.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?)$",
    "testPathIgnorePatterns": [
      "Mock.ts",
      "script.test.ts",
      "script-error.test.ts",
      "worker.test.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testURL": "http://localhost"
  }
}
